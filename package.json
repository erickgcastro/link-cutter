    "@types/react-dom": "18.0.10",
    "argon2": "^0.30.3",
    "axios": "^1.2.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "firebase": "^9.16.0",
    "next": "13.1.1",
    "prisma": "^4.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^9.1.1",
    "sharp": "^0.31.3",
    "typescript": "4.9.4"
  },
