{
  "name": "cut-link",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db-generate": "npx prisma generate --schema=./src/infra/prisma/schema.prisma",
    "db:local-migrate": "dotenv -e .env.local -- npx prisma migrate dev --schema=./src/infra/prisma/schema.prisma",
    "dev": "next dev",
    "build": "npx prisma generate --schema=./src/infra/prisma/schema.prisma && next build",
    "start": "next start",
